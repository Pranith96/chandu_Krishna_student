
public class StringExample1 {

	public static void main(String[] args) {

		String s = "Hi"; // String laterals
		String s1 = new String("Hi"); // Heap
		String s2 = "Hi";

		// == reference check
		if (s == s1) {
			System.out.println("true");
		} else {
			System.out.println("false");
		}

		if (s == s2) {
			System.out.println("true");
		} else {
			System.out.println("false");
		}

		if (s.equals(s1)) {
			System.out.println("true");
		} else {
			System.out.println("false");
		}
	}

}
